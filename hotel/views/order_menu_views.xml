<?xml version="1.0"?>
<odoo>
    <record id="hotel_order_menu_action" model="ir.actions.act_window">
        <field name="name">Products</field>
        <field name="res_model">order.menu</field>
        <field name="view_mode">tree</field>
    </record>

    <record id="tree_view_order_menu" model="ir.ui.view">
        <field name="model">order.menu</field>
        <field name="arch" type="xml">
            <tree string="Vehicle Tags" editable="bottom" >
                <field name="name"/>
                <field name="category"/>


            </tree>
        </field>
    </record>

</odoo>