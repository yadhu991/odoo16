<?xml version="1.0"?>
<odoo>

    <record id="seq_hotel" model="ir.sequence">
        <field name="name">Patient Sequence</field>
        <field name="code">hotel.property</field>
        <field name="prefix">HM/ACC/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_room" model="ir.sequence">
        <field name="name">Patient Sequence</field>
        <field name="code">hotel.room</field>
        <field name="prefix">ROOM/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_order" model="ir.sequence">
        <field name="name">Order sequence</field>
        <field name="code">order.food</field>
        <field name="prefix">ORDER/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
     <record id="seq_invoice" model="ir.sequence">
        <field name="name">invoice sequence</field>
        <field name="code">hotel.invoice</field>
        <field name="prefix">INV/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>